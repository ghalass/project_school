generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["relationJoins"]
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id       Int          @id @default(autoincrement())
  name     String
  lastName String?
  email    String       @unique
  password String
  role     UserRoleType @default(USER)
  active   Boolean      @default(true)
}

enum UserRoleType {
  SUPER_ADMIN
  ADMIN
  AGENT_SAISIE
  USER
}
